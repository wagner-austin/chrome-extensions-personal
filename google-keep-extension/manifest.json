{
  "manifest_version": 3,
  "name": "Open Google Keep",
  "version": "1.0",
  "description": "Click to open Google Keep.",
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs"]
}
